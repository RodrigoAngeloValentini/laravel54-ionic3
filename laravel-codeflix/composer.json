{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "barryvdh/laravel-cors": "0.9.2",
        "codeedu/code_validator": "0.0.2",
        "dingo/api": "1.0.0-beta8",
        "doctrine/dbal": "~2.3",
        "folklore/image": "0.3.19",
        "graham-campbell/exceptions": "^9.3",
        "jrean/laravel-user-verification": "4.1.9",
        "kris/laravel-form-builder": "1.11.0",
        "laravel/framework": "5.4.*",
        "laravel/socialite": "3.0.6",
        "laravel/tinker": "~1.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/fractal": "0.16.0",
        "patricktalmadge/bootstrapper": "5.*",
        "paypal/rest-api-sdk-php": "1.11",
        "prettus/l5-repository": "2.6.18",
        "tymon/jwt-auth": "dev-develop#9f759fe9e3112e1de50920c05537d752780cf966"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.3",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "^1.1",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "CodeFlix\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
